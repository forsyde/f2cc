# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=debug
endif
exinterface config

PROJECTS := TiCPP

exinterface TICPPOBJPATH := $(OBJPATH)/ticpp

.PHONY: all clean help $(PROJECTS) $(TICPPOBJPATH)

all: $(PROJECTS)

$(TICPPOBJPATH):
	@mkdir -p $@

TiCPP: 
	@printf $(subst %,TiCPP,$(PREBUILDMSG))
	@${MAKE} --no-print-directory -C . -f TiCPP.make
	@printf $(POSTBUILDMSG)

clean:
	@${MAKE} --no-print-directory -C . -f TiCPP.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   TiCPP"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
